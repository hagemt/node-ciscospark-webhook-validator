{
  "name": "ciscospark-webhook-validator",
  "version": "1.0.0",
  "description": "Use co-body and dataloader to validate incoming webhooks from Cisco Spark",
  "author": "Tor E Hagemann <tor.hagemann@gmail.com>",
  "homepage": "https://github.com/hagemt/node-ciscospark-webhook-validator#readme",
  "license": "MIT",
  "main": "es6.js",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.6"
          }
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/hagemt/node-ciscospark-webhook-validator/issues"
  },
  "dependencies": {
    "co-body": "5.1.1",
    "dataloader": "1.4.0",
    "node-fetch": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-node": "6.0.0",
    "mocha": "5.0.1",
    "ngrok": "2.2.26",
    "nock": "9.2.1",
    "nyc": "11.4.1",
    "request": "2.83.0",
    "should": "13.2.1",
    "sinon": "4.4.1",
    "uuid": "3.2.1"
  },
  "engines": {
    "node": ">=6.6.0"
  },
  "keywords": [
    "Cisco",
    "Spark",
    "validate",
    "webhook",
    "parse",
    "sha1",
    "hmac"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hagemt/node-ciscospark-webhook-validator.git"
  },
  "scripts": {
    "babel": "babel es6.js -o es5.js",
    "coverage": "nyc mocha --require should",
    "eslint": "eslint .eslintrc.js es6.js extras test --report-unused-disable-directives",
    "posttest": "npm run-script eslint -- --max-warnings 0",
    "prepublishOnly": "npm run-script babel",
    "pretest": "npm run-script eslint -- --fix",
    "test": "npm run coverage"
  }
}
